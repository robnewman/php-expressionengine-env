<VirtualHost *:80>
  ServerName <%= @params[:iris_servername] %>
  ServerAdmin <%= @params[:iris_serveradmin] %>
  DocumentRoot <%= @params[:iris_docroot] %>
  CustomLog  <%= @params[:iris_customlog_path] %> <%= @params[:iris_customlog_format] %>
  ErrorLog  <%= @params[:iris_errorlog_path] %>

  <Directory <%= @params[:iris_docroot] %>>
    Options FollowSymLinks +Indexes +Includes -ExecCGI
    AllowOverride FileInfo Options
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  <Directory />
    Options FollowSymLinks
    AllowOverride None
  </Directory>

</VirtualHost>
